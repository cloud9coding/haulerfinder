<h1>Listing bid_requests</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Pick up street</th>
      <th>Pick up city state</th>
      <th>Pick up zip</th>
      <th>Drop off street</th>
      <th>Drop off city state</th>
      <th>Drop off zip</th>
      <th>Pick up date</th>
      <th>Pick up time</th>
      <th>Drop off date</th>
      <th>Drop off time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bid_requests.each do |bid_request| %>
      <tr>
        <td><%= bid_request.user %></td>
        <td><%= bid_request.pick_up_street %></td>
        <td><%= bid_request.pick_up_city_state %></td>
        <td><%= bid_request.pick_up_zip %></td>
        <td><%= bid_request.drop_off_street %></td>
        <td><%= bid_request.drop_off_city_state %></td>
        <td><%= bid_request.drop_off_zip %></td>
        <td><%= bid_request.pick_up_date %></td>
        <td><%= bid_request.pick_up_time %></td>
        <td><%= bid_request.drop_off_date %></td>
        <td><%= bid_request.drop_off_time %></td>
        <td><%= link_to 'Show', bid_request %></td>
        <td><%= link_to 'Edit', edit_bid_request_path(bid_request) %></td>
        <td><%= link_to 'Destroy', bid_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bid request', new_bid_request_path %>
